<div class="masthead">
  <div class="masthead__inner-wrap">
    <div class="masthead__menu">
      <nav id="site-nav" class="greedy-nav">
        <button><div class="navicon"></div></button>
        <ul class="visible-links">
          <li class="masthead__menu-item masthead__menu-item--lg"><a href="/">Rubi Jihantoro</a></li>
        </ul>
        <ul class="hidden-links hidden"></ul>
      </nav>
    </div>
  </div>
</div>
<div id="main" role="main">
  <div class="sidebar sticky">
<div itemscope itemtype="http://schema.org/Person">
  <div class="author__avatar">
    	<img src="/rubi-jihantoro.png" class="author__avatar" alt="Rubi Jihantoro"> 
  </div>
  <div class="author__content">
    <h3 class="author__name">Rubi Jihantoro</h3>
    <p class="author__bio">Polyglot, Developer, Book Writer, Startup Boy</p>
  </div>
  <div class="author__urls-wrapper">
    <button class="btn btn--inverse">Follow</button>
    <ul class="author__urls social-icons">
      <li><i class="fa fa-fw fa-map-marker" aria-hidden="true"></i> Yogyakarta</li>
      <li><a target="_blank" href="https://www.linkedin.com/in/rubi-jihantoro/"><i class="fa fa-fw fa-linkedin-square" aria-hidden="true"></i> Linkedin</a></li>
      <li><a target="_blank" href="https://www.facebook.com/real.jihantoro"><i class="fa fa-fw fa-facebook-square" aria-hidden="true"></i> Facebook</a></li>
      <li><a target="_blank" href="https://twitter.com/codenoid"><i class="fa fa-fw fa-twitter-square" aria-hidden="true"></i> Twitter</a></li>
      <li><a target="_blank" href="https://github.com/codenoid"><i class="fa fa-fw fa-github" aria-hidden="true"></i> Github</a></li>
    </ul>
  </div>
</div>
</div>
  <div class="archive">
    <h1 class="page__title"></h1>
    <h3 class="archive__subtitle">Recent Posts</h3>
<% rd.each do %>
<% read = rd.read(String) %>
<% url = rd.read(String) %>
<% data = read.split("<~|~>") %>
<div class="list__item">
  <article class="archive__item" itemscope itemtype="http://schema.org/CreativeWork">
    <h2 class="archive__item-title" itemprop="headline">
      <a href="/read/<%= url %>" rel="permalink"><%= data[0] %></a> 
    </h2>
    <p class="page__meta"><i class="fa fa-clock-o" aria-hidden="true"></i> 
	   <%= reading_time(data[1].size) %> Minute Read
    </p>
    <p class="archive__item-excerpt" itemprop="description"><%= data[1][0...80] %></p>
  </article>
</div>
<% end %>
<nav class="pagination" style="display: none;">
  <ul>
